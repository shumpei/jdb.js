/*
path, '<', '=', '>' 'and' literal

path: javascript_identifer*
javascript_identifer: 'a-zA-Z0-9_'+
literal: number_literal | string_literal | '?'
number_literal: 0-9*
string_literal: 'any' | "any"
expr: path '<' literal |
  path '>' literal |
  path '=' literal |
  path '<=' literal |
  path '>=' literal |
  literal '<' path |
  literal '>' path |
  literal '=' path |
  literal '<=' path |
  literal '>=' path |
  expr 'and' expr

*/